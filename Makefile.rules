PREPROCESS.c = $(CC) $(CPPFLAGS) $(TARGET_MACH) -E

COMPILE.c = $(CC) $(CCFLAGS) $(CPPFLAGS) $(TARGET_MACH) -S

COMPILE.S = $(CC) $(ASFLAGS) $(CPPFLAGS) $(TARGET_MACH) -S

ASSEMBLE.S = $(CC) $(ASFLAGS) $(CPPFLAGS) $(TARGET_MACH) -c


%.S: %.i
	$(COMPILE.c) $(OUTPUT_OPTION) $<

%.o: %.S
	$(ASSEMBLE.S) -o $@ $<

